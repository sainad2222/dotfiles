	  "ternary search": {
		"prefix": "ternary",
		"body": [
		  "def ternarySearch(arr,x,l,r):",
		  "    while(r>=l):",
		  "        mid1=l+(r-l)//3",
		  "        mid2=r-(r-l)//3",
		  "        if(x==arr[mid1]):",
		  "            return mid1",
		  "        elif(x==arr[mid2]):",
		  "            return mid2",
		  "        if(x<arr[mid1]):",
		  "            r=mid1-1",
		  "        elif(x>arr[mid2]):",
		  "            l=mid2+1",
		  "        else:",
		  "            l=mid1+1",
		  "            r=mid2-1",
		  "    return -1"
		],
		"description": "ternary search"
	  },
	  "Seive": {
		"prefix": "primeSeive",
		"body": [
		  "def SieveOfEratosthenes(n): ",
		  "    prime = [True for i in range(n + 1)] ",
		  "    p = 2",
		  "    while (p * p <= n): ",
		  "        if (prime[p] == True): ",
		  "            for i in range(p * 2, n + 1, p): ",
		  "                prime[i] = False",
		  "        p += 1",
		  "    prime[0]= False",
		  "    prime[1]= False ",
		  "    for p in range(n + 1): ",
		  "        if prime[p]: ",
		  "            print(p)"
		],
		"description": "Seive"
